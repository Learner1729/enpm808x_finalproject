<launch> 
  <!-- Open turtlebot world -->
  <include file="$(find turtlebot_gazebo)/launch/turtlebot_world.launch">
    <arg name="world_file" value="$(find naivik_robot)/world/customWorld" />
  </include>

  <!-- Open amcl demo world -->
  <include file="$(find turtlebot_gazebo)/launch/amcl_demo.launch">
    <arg name="map_file" value="$(find naivik_robot)/map/custom_world_map.yaml" />
  </include>

  <!-- Open RVIZ -->
  <!-- include file="$(find turtlebot_rviz_launchers)/launch/view_navigation.launch" -->
  <node pkg="rviz" name="rviz" type="rviz" args="-d $(find naivik_robot)/rviz/gmapping.rviz"/>
</launch>